{
  "name": "k8s-tool",
  "version": "0.1.4",
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "tagName": "v${version}"
    }
  },
  "description": "Boozang k8s learning",
  "main": "server.js",
  "bin": {
    "k8s-tool": "server.js"
  }, 
  "scripts": {
    "start": "node server.js"
  },
  "keywords": [
    "Node.js"
  ],
  "author": "Wensheng Li and Mats Ljunggren",
  "license": "MIT",
  "pkg": {
    "scripts":["public/**/*.js","server/*.js"],
    "assets":["public/*.html","public/css/*.css","public/font/*","node_modules/socket.io/client-dist/*.js"],
    "outputPath": "../artifacts/k8s-tool"
  },
  "dependencies": {
    "mkdirp": "^1.0.4",
    "express": "latest",
    "socket.io": "^4.1.2"
  }
}
